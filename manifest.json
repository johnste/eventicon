{
  "name": "SocIcon",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Update the favicon of sites with notifications",  
  "content_scripts": [
    {
      "matches": ["*://facebook.com/*", "*://twitter.com/*", "*://linkedin.com/*"],
      "js": ["tinycon.min.js", "socicon.js"]
    }
  ]
}